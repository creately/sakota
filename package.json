{
  "name": "@creately/sakota",
  "version": "0.0.0",
  "description": "Proxies js objects and records all changes made on an object without modifying the object.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf coverage dist",
    "build": "tsc",
    "lint": "prettier --print-width 120 --single-quote --trailing-comma es5 --write \"src/**/*.ts\"",
    "test": "karma start karma.conf.js --single-run",
    "test:watch": "karma start karma.conf.js",
    "test:cover": "npm run test && open coverage/index.html",
    "prepublish": "npm run clean && npm run lint && npm run build"
  },
  "author": "Muhammed Thanish <mnmtanish@gmail.com> (http://thanish.me/)",
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "^2.8.9",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "^3.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^1.1.2",
    "karma-webpack": "^3.0.5",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "ts-loader": "^5.2.1",
    "typescript": "^3.1.1",
    "webpack": "^4.20.2"
  }
}
